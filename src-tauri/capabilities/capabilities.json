{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "CheetahNote 默认权限配置",
  "windows": ["*"],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-create",
    "core:window:allow-center",
    "core:window:allow-set-title",
    "core:webview:default",
    "core:app:default",
    "shell:default",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-ask",
    "dialog:allow-confirm"
  ]
}